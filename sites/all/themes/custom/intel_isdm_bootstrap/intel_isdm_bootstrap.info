regions[page_top]       = 'Page top'
regions[page_bottom]    = 'Page bottom'


;;;;;;;;;;;;;;;;;;;;;
;; Stylesheets
;;;;;;;;;;;;;;;;;;;;;

stylesheets[all][] = css/yamm.css
stylesheets[all][] = css/style.css
stylesheets[all][] = css/jquery-ui.css
stylesheets[all][] = css/custom.css


;;;;;;;;;;;;;;;;;;;;;
;; Scripts
;;;;;;;;;;;;;;;;;;;;;

scripts[] = 'node_modules/bootstrap-sass/assets/javascripts/bootstrap.min.js'
scripts[] = 'js/scripts.js'
scripts[] = 'js/config.js'
scripts[] = 'js/solutions-facet-sidebar.js'
scripts[] = 'js/prodcompare.js'

; Disable BootstrapCDN so the compiled SASS source files can be used.
settings[bootstrap_cdn] = ''

;;;;;;;;;;;;;;;;;;;;;
;; Additional settings
;;;;;;;;;;;;;;;;;;;;;

plugins[panels][layouts] = panels_layouts
